<div class="login-user-form">
  <div class ="login-button">
    <h1>Marina & Lawrence</h1>
    <h2>June 2, 2018</h2>
    <%= form_for(:session, url: login_path) do |f| %>
      <div class="code-label">
        <%= f.label :code, 'Password:' %>
        <%= f.password_field :code %>
        <%= f.submit "Submit", :class => 'submit-button'%>
        </div>
      </div>
    <% end %>
  </div>
</div>

